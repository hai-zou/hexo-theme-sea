<div class="sea-category-wrapper">
  <div class="sea-page-card-wrapper">
    <div class="sea-page-card-title">
      {{ page.category }}
    </div>

    {% for post in page.posts.toArray() %}
      <div class="sea-category-post-item">
        <a
          class="sea-category-post-title"
          href="{{ url_for(post.path) }}"
        >
          {{ post.title }}
        </a>
        {{ time_tag(post.date) }}
      </div>
    {% endfor %}
  </div>

  {{ partial('partial/pagination') }}
</div>