<div class="sea-menu-wrap">
  {% for menu_item in theme.menu %}
    <a
      class="sea-menu-link"
      {% if menu_item.url.startsWith("http") %}
        target="_blank"
      {% endif %}
      href="{{ url_for(menu_item.url) }}"
    >
      {{ menu_item.name }}
    </a>
  {% endfor %}

  <span class="sea-menu-sep">|</span>

  {{ partial('_plugins/search/index') }}

  <span class="sea-menu-icon" id="sea-theme-dark">
    {{ partial('svg/moon') }}
  </span>
  <span class="sea-menu-icon" id="sea-theme-light">
    {{ partial('svg/sun') }}
  </span>

  <span id="sea-menu-close-icon">
    {{ partial('svg/close') }}
  </span>
</div>
<div id="sea-nav-dimmer"></div>